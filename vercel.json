{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "api/predict.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    },
    "api/fraud-detection.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/fraud-detection",
      "dest": "/api/fraud-detection.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHON_EXECUTABLE": "python3"
  },
  "outputDirectory": ".next"
}
