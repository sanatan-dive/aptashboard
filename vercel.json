{
  "functions": {
    "api/predict.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    },
    "api/fraud-detection.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/fraud-detection",
      "dest": "/api/fraud-detection.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
